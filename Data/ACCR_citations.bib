    author = {Louis-Marie Charbonnier and Erin Janssen and Janet Chou and Toshiro K Ohsumi and Sevgi Keles and Joyce T Hsu and Michel J Massaad and Maria Garcia-Lloret and Rima Hanna-Wakim and Ghassan Dbaibo and Abdullah A Alangari and Abdulrahman Alsultan and Daifulah Al-Zahrani and Raif S Geha and Talal A Chatila},
    journal = {{J. Allergy Clin. Immunol.}},
    volume = {135},
    number = {1},
    year = {2015},
    month = jan,
    pages = {217-27},
