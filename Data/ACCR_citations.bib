    nlmuniqueid = {9010756}
}

@article{pmid27803417,
    title = {{Fibromyalgia in a Patient with Cushing's Disease Accompanied by Central Hypothyroidism.}},
    author = {Nobumasa Ohara and Shinichi Katada and Takaho Yamada and Naomi Mezaki and Hiroshi Suzuki and Akiko Suzuki and Osamu Hanyu and Yuichiro Yoneoka and Izumi Kawachi and Takayoshi Shimohata and Akiyoshi Kakita and Masatoyo Nishizawa and Hirohito Sone},
