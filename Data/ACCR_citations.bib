@article{pmid28137718,
    title = {{Bacterial meningitis: a rare complication of an unrecognised nasal foreign body in a child.}},
    author = {Jana van der Veen and Simon Thorne},
    journal = {{BMJ Case Rep}},
    volume = {2017},
    year = {2017},
    month = jan,
    pages = {},
    abstract = {We present an interesting case of a 7-year-old child who developed severe bacterial meningitis requiring admission to paediatric ICU. Initial investigations failed to identify the reason for this otherwise healthy child to develop this life-threatening condition. The child recovered fully with no long-term sequelae. Further on-going unilateral purulent rhinorrhoea lead to ENT referral. CT head from a previous admission showed mucosal thickening and fluid within paranasal sinuses. Reluctance to expose the child to further radiation meant that no formal scan of sinuses was organised and the child was diagnosed with chronic rhinosinusitis. Symptoms failed to improve despite the treatment. A subsequent CT scan of sinuses revealed a radiopaque foreign body in the nasal cavity. This is a rare presentation of a nasal foreign body causing bacterial meningitis. This case highlights the importance of comprehensive investigations to avoid delays in establishing a correct diagnosis and initiating the appropriate treatment.},
    pubmed = {28137718},
    pii = {bcr-2015-209577},
