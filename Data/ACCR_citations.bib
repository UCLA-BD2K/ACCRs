@article{pmid1648309,
    title = {{Renal and skin involvement in a patient with complete Kearns-Sayre syndrome.}},
    author = {K Mori and K Narahara and S Ninomiya and Y Goto and I Nonaka},
    journal = {{Am. J. Med. Genet.}},
    volume = {38},
    number = {4},
    year = {1991},
