@article{pmid22892509,
    title = {{A case of HELLP syndrome with multiple complications.}},
    author = {Yoshiaki Iwashita and Tomomichi Kan'o and Jun Hattori and Shingo Konno and Hiroshi Imai and Takao Kitahara and Kazui Soma},
    journal = {{Intern. Med.}},
    volume = {51},
    number = {16},
    year = {2012},
    pages = {2227-30},
    abstract = {Hemolysis, elevated liver enzymes, and low platelet (HELLP) syndrome is a rare complication of pregnancy. The mortality rate associated with HELLP syndrome increases when life-threatening complications occur. A 37-year-old woman at 37 weeks of gestation developed severe cerebral hemorrhage at the beginning of labor induction and was transferred to our hospital, where HELLP syndrome was diagnosed. She developed disseminated intravascular coagulation (DIC), hepatic hematoma, and cerebral infarction after surgery. On day 68, she was transferred to her local hospital. Careful observation and rapid management can save patients with severe complications of HELLP syndrome.},
