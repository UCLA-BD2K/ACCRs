    author = {Guilherme Abdalla and Edson Marchiori and Gláucia Zanetti and Antonio Mucillo and Mariana Leite Pereira and Nina Ventura and Pedro Martins and Carolina Pesce Lamas Constantino and Rodrigo Canellas and Viviane Brandão and Romulo Varella de Oliveira},
