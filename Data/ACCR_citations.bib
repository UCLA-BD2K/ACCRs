    journal = {{Arch. Ophthalmol.}},
    volume = {98},
    number = {10},
    year = {1980},
    month = oct,
    pages = {1764-6},
    abstract = {An elderly woman from Minnesota had bilateral bulbar conjunctival nodules that proved to be caused by Mycobacterium bovis. She was found to have active military tuberculosis; only pulmonary and ocular involvement was confirmed. She had a history of tuberculosis but no recent exposure or intercurrent illness. The case was unusual in that the endogenous infection caused bilateral bulbar symptoms but no palpable lymphadenopathy and no other organ involvement. This case demonstrates many of the basic tenents on the course of tuberculosis in humans. Triple chemotherapy was administered, and the patient had a slow but definite clinical response.},
    pubmed = {7000052},
    nlmuniqueid = {7706534}
}

@article{pmid11995849,
