@article{pmid25295501,
    title = {{Response and acquired resistance to everolimus in anaplastic thyroid cancer.}},
    author = {Nikhil Wagle and Brian C Grabiner and Eliezer M Van Allen and Ali Amin-Mansour and Amaro Taylor-Weiner and Mara Rosenberg and Nathanael Gray and Justine A Barletta and Yanan Guo and Scott J Swanson and Daniel T Ruan and Glenn J Hanna and Robert I Haddad and Gad Getz and David J Kwiatkowski and Scott L Carter and David M Sabatini and Pasi A Jänne and Levi A Garraway and Jochen H Lorch},
    journal = {{N. Engl. J. Med.}},
